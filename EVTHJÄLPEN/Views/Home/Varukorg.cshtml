@model ViewProducts
@{
    ViewData["Title"] = "Varukorg";
}
<h1>@ViewData["Title"]</h1>
<a class="btn btn-primary" href="#" role="button">Fortsätt handla </a>

<div class="col-md-6 " (style="border-left-width:)">
    @foreach (var item in Model.Productslist)
    {<ul class="align-content-lg-stretch">
            <li class="list-group-item">
                <p> @item.ProductName <span style="color: red">@item.Quantity</span> <span style="color: aqua">@item.Price</span>:-</p>
            </li>
     </ul>}
</div>


<button class="btn btn-primary" type="submit">Checka ut</button>
<input class="btn btn-primary" type="button" value="Befräfta order">




